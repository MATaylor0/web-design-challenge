    "# Exporting a pandas dataframe to a HTML file to be displayed on a webpage\n",
    "import pandas as pd\n",
    "\n",
    "path = \"../Resources/city_data.csv\"\n",
    "\n",
    "city_df = pd.read_csv(path)\n",
    "\n",
    "output = \"../Resources/city-data.html\"\n",
    "\n",
    "city_df.to_html(output)"
